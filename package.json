{
  "name": "iqy-server",
  "version": "0.2.5",
  "description": "a web framework by koa and typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "release": "npm publish --access public",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "eslint .  --ext .ts",
    "prettier": "prettier --write '**/*.{js,json,md,ts}'",
    "test": "mocha --timeout 10000 --require ts-node/register \"./test/**/*.test.ts\" --exit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "koa",
    "typescript",
    "iqy-server",
    "framework"
  ],
  "author": "SouthOrange",
  "license": "MIT",
  "dependencies": {
    "@types/request": "^2.48.4",
    "@types/koa": "^2.11.0",
    "@types/koa-router": "^7.0.42",
    "iqy-mysql": "^0.0.5",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-router": "^7.4.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "eslint-config-iqyserver": "^0.0.5",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "mocha": "^6.2.2",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "vuepress": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/southorange1228/iqy-server.git"
  }
}